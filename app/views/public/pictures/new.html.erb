<h1>Public::Pictures#new</h1>
<%= form_with model: @picture, url: public_pictures_path, method: :post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :scene_image %>
  <%= f.file_field :scene_image, accept: "image/*" %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :produce %>
  <%= f.text_area :produce %>
  <%= f.submit "投稿" %>
<% end %>